{
  "manifest_version": 3,
  "name": "KeyCrypt",
  "version": "0.0.1",
  "description": "Encrypt Password based on your key",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "KeyCrypt",
    "default_popup": "popup.html",
    "default_icon": "assets/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost/*"],
      "js": ["popup.js"]
    }
  ]
}
